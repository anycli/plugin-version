{
  "name": "@anycli/version-plugin",
  "description": "a anycli command that just shows the CLI version",
  "version": "0.1.19",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init"
    }
  },
  "bugs": "https://github.com/anycli/version-plugin/issues",
  "dependencies": {
    "@anycli/command": "^0.3.4",
    "cli-ux": "^3.3.12"
  },
  "devDependencies": {
    "@anycli/config": "^0.2.11",
    "@anycli/engine": "^0.1.44",
    "@anycli/test": "^0.10.2",
    "@anycli/tslint": "^0.2.2",
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.100",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.2",
    "@types/node": "^9.4.0",
    "@types/node-notifier": "^0.0.28",
    "@types/read-pkg": "^3.0.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.16.0",
    "eslint-config-anycli": "^1.3.1",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/anycli/version-plugin",
  "keywords": [
    "anycli-plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "anycli/version-plugin",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"eslint .\" \"tsc -p test --noEmit\" \"tslint -p test\"",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
